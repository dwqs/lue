{
  "name": "lue",
  "version": "0.0.1",
  "description": "Vue and vuex based framework, writing less verbose code.",
  "author": "pomysky@gmail.com",
  "main": "dist/lue.min.js",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/dwqs/lue"
  },
  "keywords": [
    "vue",
    "vuejs2",
    "vuex",
    "vue-router",
    "vuex router sync",
    "vuex sync"
  ],
  "scripts": {
    "test": "./node_modules/.bin/ava ./test/**/*.js",
    "dev": "node ./build/dev-server.js",
    "prepush": "npm run ilint -q",
    "prepublishOnly": "npm run build",
    "build": "npx webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "ilint": "npx eslint src/**/*.js",
    "fix": "npx eslint --fix src/**/*.js"
  },
  "dependencies": {
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "async-await-error-handling": "^0.5.2",
    "ava": "^0.22.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "open-browser-webpack-plugin": "^0.0.5",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.0",
    "vue-router": "^2.7.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-parallel-uglify-plugin": "^0.4.2"
  },
  "homepage": "https://github.com/dwqs/lue",
  "bugs": {
    "url": "https://github.com/dwqs/lue/issues"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.2.0"
  }
}
